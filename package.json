{
	"name": "report-generator",
	"version": "1.0.0",
	"win": {
		"icon": "./electron-src/assets/logo.ico"
	},
	"author": {
		"name": "Ezequiel Augusto Stanganelli",
		"email": "ezequielstanganelli@gmail.com"
	},
	"description": "A minimal Electron application written with Typescript",
	"main": "main/index.js",
	"scripts": {
		"clean": "rimraf dist main renderer/out renderer/.next",
		"start": "npm run build && electron .",
		"build-renderer": "next build renderer",
		"build-electron": "tsc -p electron-src",
		"build": "npm run clean && npm run build-renderer && npm run build-electron",
		"pack-app": "npm run build && electron-builder --dir",
		"dist": "npm run build && electron-builder build --win",
		"dist:debug": "lldb ./dist/mac/electron-quick-start-typescript.app",
		"dist:unpack": "npx asar extract ./dist/mac/electron-quick-start-typescript.app/Contents/Resources/app.asar ./dist/unpack",
		"type-check": "tsc"
	},
	"dependencies": {
		"@ant-design/icons": "^5.3.0",
		"@react-pdf/renderer": "^3.1.12",
		"antd": "^5.14.2",
		"electron-is-dev": "^1.1.0",
		"electron-settings": "^4.0.2",
		"mysql2": "^3.9.1",
		"next": "^12.0.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-pdf-charts": "^0.2.5",
		"recharts": "^2.12.2"
	},
	"devDependencies": {
		"@types/node": "^14.14.6",
		"@types/react": "^18.2.20",
		"electron": "^29.1.0",
		"electron-builder": "^24.13.3",
		"rimraf": "^3.0.0",
		"typescript": "^4.5.4"
	},
	"build": {
		"asar": true,
		"files": [
			"main",
			"renderer"
		]
	}
}